<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/css/header.css" />
  </head>
  <body>
    <!-- header start -->
    <div class="header">
      <div class="header_wrap">
        <div class="logo"><a href="/">ohstory</a></div>
        <div class="sign">
          <% if(loginchk.user_id === undefined){ %>
          <a href="/login">login</a>
          <a href="/signup">signup</a>
          <% } else { %>
          <a href="/logout/<%= loginchk.user_id %>"><%= loginchk.user_id %></a>

          <% } %>
        </div>
      </div>
    </div>
    <!-- header end -->
  </body>
</html>
